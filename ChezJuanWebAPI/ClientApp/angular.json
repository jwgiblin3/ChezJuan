{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "75ae9d6d-e9db-4f0e-9da9-056fa8c47301"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ChezJuanWebAPI": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "progress": false,
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/assets/images/icons/favicon.png",
              "src/assets"
            ],
            "styles": [
              "src/assets/css/plugins/bootstrap.min.css",
              "src/assets/css/plugins/animate.min.css",
              "src/assets/css/plugins/magnific-popup.css",
              "src/assets/css/plugins/slick.css",
              "src/assets/css/plugins/slick-theme.css",
              "src/assets/fonts/flaticon/flaticon.css",
              "src/assets/fonts/font-awesome/css/all.min.css",
              "src/assets/css/style.css",
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/js/plugins/jquery-3.4.1.min.js",
              "src/assets/js/plugins/popper.min.js",
              "src/assets/js/plugins/waypoint.js",
              "src/assets/js/plugins/bootstrap.min.js",
              "src/assets/js/plugins/jquery.magnific-popup.min.js",
              "src/assets/js/plugins/jquery.slimScroll.min.js",
              "src/assets/js/plugins/imagesloaded.min.js",
              "src/assets/js/plugins/jquery.steps.min.js",
              "src/assets/js/plugins/jquery.countdown.min.js",
              "src/assets/js/plugins/isotope.pkgd.min.js",
              "src/assets/js/plugins/slick.min.js",
              "src/assets/js/main.js",
              "src/assets/js/main.js"
            ],
            "aot": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ChezJuanWebAPI:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ChezJuanWebAPI:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ChezJuanWebAPI:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": ["src/styles.css"],
            "scripts": [],
            "assets": ["src/assets"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist-server",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.server.json"
          },
          "configurations": {
            "dev": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true
            }
          }
        }
      }
    },
    "ChezJuanWebAPI-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ChezJuanWebAPI:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "ChezJuanWebAPI"
}
